#include<bits/stdc++.h>
using namespace std; 

void solve(){   
 
 int n,x;
 scanf("%d%d",&n,&x);
 int arr[n];
 for(int i=0;i<n;i++){
    scanf("%d",&arr[i]);
 }
 sort(arr,arr+n);
 for(int i=0;i<n;i++){
  for(int j=0;j<n;j++){
    if(abs(arr[i]-arr[j])==x){
      cout<<"1"<<endl;
      return;
    }
    else if(abs(arr[i]-arr[j])<x){
          break;
    }
  }
 }
 cout<<"-1"<<endl;
 return;
}
int main()
{
 solve();
 return 0; 
}

 
