#include<bits/stdc++.h>

using namespace std; 

void solve(){
 
 int n,x;
 scanf("%d%d",&n,&x);
 int arr[n];
 for(int i=0;i<n;i++){
    scanf("%d",&arr[i]);
  }
  int flag = 0;
  for(int i=0;i<n;i++){
    if(arr[i]==x){
      cout<<i<<" ";
      flag = 1;
      break;
    } 
  }
 if(flag==0){cout<<"-1"<<endl; return;}
  for(int i=n-1;i>=0;i--){
    if(arr[i]==x){
      cout<<i<<endl;
      break;  
    } 
  }

}
int main()
{
 int t;
 scanf("%d",&t);
 while(t--){
solve();}
 return 0; 
}

 
