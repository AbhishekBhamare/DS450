#include<bits/stdc++.h>

using namespace std; 

void solve(){  
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
      scanf("%d",&arr[i]);
    }
    
    next_permutation(arr,arr+n);
    for(int i=0;i<n;i++){
      printf("%d ",arr[i]);
    }
    printf("\n");

   }
int main()
{
  solve();
  return 0;
}
